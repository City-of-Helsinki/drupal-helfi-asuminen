#
# Enviromental values for Makefile and for docker-compose.yml
#

# Docker Compose project name
COMPOSE_PROJECT_NAME=asuminen

# OpenShift project name
OC_PROJECT_NAME=hki-kanslia-asuminen-test

# Stage file proxy origin domain
STAGE_FILE_PROXY_ORIGIN=https://asuminen.blob.core.windows.net

# Stage file proxy path
STAGE_FILE_PROXY_ORIGIN_DIR=

# Local hostname
DRUPAL_HOSTNAME=helfi-asuminen.docker.so

# Docker image
DRUPAL_IMAGE=ghcr.io/city-of-helsinki/drupal-web:8.0

# Default source @alias where to sync database and files from
DRUPAL_SYNC_SOURCE=main

# Public webroot
DRUPAL_WEBROOT=public
